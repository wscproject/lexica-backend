{
  "name": "wiki-lexica",
  "version": "1.0.0",
  "description": "Lexica",
  "private": true,
  "scripts": {
    "start": "nodemon --exec npm run babel-node -- ./app/bin/www -x",
    "babel-node": "babel-node --presets=@babel/preset-env",
    "build-server": "babel -d ./build ./app -s",
    "frontend:install": "cd ../lexica-frontend && npm install",
    "frontend:build": "cd ../lexica-frontend && npm run build",
    "frontend:copy": "cp -r ../lexica-frontend/dist/* dist/",
    "pm2-prod": "pm2 start ecosystem.config.js",
    "build": "npm run build-server && npm run pm2-prod",
    "build:full": "npm run frontend:install && npm run frontend:build && npm run frontend:copy && npm run build", 
    "vercel-build": "echo vercel-deployed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@babel/node": "^7.22.5",
    "axios": "^0.25.0",
    "babel-polyfill": "^6.26.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.2",
    "joi": "^14.3.1",
    "joi-errors-for-forms": "^0.2.6",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.2",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.22",
    "pm2": "^5.3.0",
    "qs": "^6.12.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.22.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "husky": "^1.3.1",
    "mocha": "^10.2.0"
  }
}
